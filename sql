CREATE TABLE `santa_teresa`.`services` ( `id` INT NOT NULL AUTO_INCREMENT ,`title` VARCHAR(50) NOT NULL , `especialista_id` VARCHAR(50) NOT NULL , `especialidad_id` VARCHAR(50) NOT NULL , `servicio_id` VARCHAR(50) NOT NULL , `date` DATE NOT NULL , `hora_id` VARCHAR(50) NOT NULL , `created_at` TIMESTAMP NOT NULL , `updated_at` TIMESTAMP NOT NULL , PRIMARY KEY (`id`)) ENGINE = InnoDB; 